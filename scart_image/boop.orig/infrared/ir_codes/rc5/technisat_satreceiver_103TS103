  // Power	->	Standby      /*    SAT1-OPTION                0x000000000000120A          SAT1-A/B                   0x0000000000001223          SAT1-SFI                   0x000000000000122F          SAT1-EXIT                  0x0000000000001222          SAT1-INFO                  0x000000000000020F          SAT1-TV/RADIO              0x0000000000000213          SAT1-MENU                  0x0000000000000212          SAT1-TV/SAT                0x0000000000000200          SAT1-TXT                   0x000000000000123C          SAT1-STOP                  0x0000000000001229*/},