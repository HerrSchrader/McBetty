{
	0x00, // A	->
	0x00, // B	->
	0x00, // C	->
	0x00, // D	->
	0x00, // Betty	->
	0x27, // Exit	->	Back
	0x59, // Up	->	Up
	0x58, // Down	->	Down
	0x56, // Left	->	Left
	0x57, // Right	->	Right
	0xF4, // OK	->	Select
	0x00, // Vol+	->
	0x00, // Vol-	->
	0x00, // Mute	->
	0x20, // Prog+	->	Skip+
	0x22, // Prog-	->	Skip-
	0x31, // 1	->	1
	0x32, // 2	->	2
	0x33, // 3	->	3
	0x34, // 4	->	4
	0x35, // 5	->	5
	0x36, // 6	->	6
	0x37, // 7	->	7
	0x38, // 8	->	8
	0x39, // 9	->	9
	0x30, // 0	->	0
	0x15, // -/--	->	Play
	0x1F, // AV	->	Stop
	0x08, // Menu	->	Menu
	0x19, // PiP	->	Pause
	0x1C, // A/B	->	Forward
	0x1D, // 16:9	->	Reverse
	0x3C, // Info	->	Info
	0x1A, // VTX1	->	Title
	0x2A, // VTX2	->	Display
	0x00, // VTX3	->
	0x00, // Blue	->
	0x00, // Yello	->
	0x00, // Green	->
	0x00, // Red	->
	0x00, // TV	->
	0x00  // Power	->
},
