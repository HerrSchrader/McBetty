{
	0x000, // A	->
	0x000, // B	->
	0x000, // C	->
	0x000, // D	->
	0x000, // Betty	->
	0x000, // Exit	->
	0x360, // Up	->	Play
	0x330, // Down	->	Stop
	0x320, // Left	->	Rewind
	0x350, // Right	->	Forward
	0x050, // OK	->	Enter
	0x2E0, // Vol+	->	Vol+
	0x2F0, // Vol-	->	Vol-
	0x060, // Mute	->	Muting
	0x0D0, // Prog+	->	Prog+
	0x0E0, // Prog-	->	Prog-
	0x100, // 1	->	1
	0x110, // 2	->	2
	0x120, // 3	->	3
	0x130, // 4	->	4
	0x140, // 5	->	5
	0x150, // 6	->	6
	0x160, // 7	->	7
	0x170, // 8	->	8
	0x180, // 9	->	9
	0x190, // 0	->	0
	0x1F0, // -/--	->	A/B
	0x000, // AV	->
	0x3A0, // Menu	->	Info
	0x000, // PiP	->
	0x000, // A/B	->
	0x000, // 16:9	->
	0x1C0, // Info	->	VTX1
	0x1E0, // VTX1	->	VTX2
	0x3C0, // VTX2	->	VTX3
	0x020, // VTX3	->	VTX4
	0x000, // Blue	->
	0x000, // Yello	->
	0x000, // Green	->
	0x1E0, // Red	->	Record
	0x000, // TV	->
	0x010  // Power	->	Power
},
